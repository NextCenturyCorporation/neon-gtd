{
    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "databases": [{
            "name": "test",
            "prettyName": "Test",
            "tables": [{
                "name": "earthquakes",
                "prettyName": "Earthquakes",
                "fields": [{
                    "columnName": "_id",
                    "prettyName": "Database ID"
                }, {
                    "columnName": "time",
                    "prettyName": "Time"
                }, {
                    "columnName": "latitude",
                    "prettyName": "Latitude"
                }, {
                    "columnName": "longitude",
                    "prettyName": "Longitude"
                }, {
                    "columnName": "depth",
                    "prettyName": "Depth"
                }, {
                    "columnName": "mag",
                    "prettyName": "Magnitude"
                }, {
                    "columnName": "magType",
                    "prettyName": "Magnitude Type"
                }, {
                    "columnName": "nst",
                    "prettyName": "NST"
                }, {
                    "columnName": "gap",
                    "prettyName": "Gap"
                }, {
                    "columnName": "dmin",
                    "prettyName": "DMIN"
                }, {
                    "columnName": "rms",
                    "prettyName": "RMS"
                }, {
                    "columnName": "net",
                    "prettyName": "Net"
                }, {
                    "columnName": "id",
                    "prettyName": "ID"
                }, {
                    "columnName": "updated",
                    "prettyName": "Updated"
                }, {
                    "columnName": "place",
                    "prettyName": "Place"
                }, {
                    "columnName": "type",
                    "prettyName": "Type"
                }],
                "mappings": {
                    "date": "time",
                    "latitude": "latitude",
                    "longitude": "longitude",
                    "line_category": "type",
                    "bar_x_axis": "net",
                    "y_axis": "mag",
                    "color_by": "magType",
                    "size_by": "mag",
                    "sort_by": "time",
                    "count_by": "net",
                    "allCoordinates": [{
                        "latitude": "latitude",
                        "longitude": "longitude"
                    }]
                }
            }]
        }],
        "mapLayers": [{
            "database": "test",
            "table": "earthquakes",
            "type": "heatmap",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "sizeBy": "mag"
        }, {
            "database": "test",
            "table": "earthquakes",
            "active":  true,
            "type": "cluster",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "colorBy": "magType",
            "sizeBy": "mag"
        }, {
            "database": "test",
            "table": "earthquakes",
            "active":  true,
            "type": "points",
            "latitudeMapping": "latitude",
            "longitudeMapping": "longitude",
            "limit": 5000,
            "colorBy": "magType",
            "sizeBy": "mag"
        }],
        "mapConfig": {
            "bounds" : {
                "top": 90,
                "bottom": -90,
                "left": -180,
                "right": 180
            }
        },
        "linkyConfig": {
          "mentions": false,
          "hashtags": false,
          "urls": true,
          "linkTo": "twitter"
        },
        "options": {
            "colorMaps": {
                "test": {
                    "earthquakes": {
                        "myField": {
                            "fieldA": "#123456",
                            "fieldB": "#654321"
                        }
                    }
                }
            },
            "mapBaseLayer": {
                "color": "dark",
                "protocol": "https"
            }
        }
    }],

    "layouts": {
        "default": [{
            "sizeX": 18,
            "minSizeX": 12,
            "sizeY": 4,
            "minSizeY": 4,
            "type": "timeline-selector"
        }, {
            "sizeX": 18,
            "sizeY": 4,
            "type": "map"
        }, {
            "sizeX": 18,
            "sizeY": 4,
            "type": "query-results-table"
        }, {
            "sizeX": 18,
            "sizeY": 4,
            "type": "filter-builder"
        }, {
            "sizeX": 6,
            "sizeY": 4,
            "type": "circular-heat-form"
        }, {
            "sizeX": 6,
            "sizeY": 4,
