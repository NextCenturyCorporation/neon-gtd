{
    "xdata": {
        "echoToConsole": false,
        "enableLoggin": false,
        "activityLoggerUrl": "http://xd-draper.xdata.data-tactics-corp.com:1337",
        "component": "Neon Demo",
        "componentVersion": "1.0.0-SNAPSHOT"
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "dig": {
        "enabled": false,
        "server": "http://localhost:9000"
    },

    "visualizations": [{
        "name": "Timeline",
        "sizeX": 6,
        "sizeY": 1,
        "type": "timeline-selector",
        "icon": "img/visualizations/Timeline64.png"
    }, {
        "name": "Map",
        "sizeX": 4,
        "sizeY": 2,
        "type": "heat-map",
        "icon": "img/visualizations/Map64.png"
    }, {
        "name": "Linechart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "linechart",
        "icon": "img/visualizations/LineChart64.png"
    }, {
        "name": "Barchart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "barchart",
        "icon": "img/visualizations/BarChart64.png"
    }, {
        "name": "Ops Clock",
        "sizeX": 2,
        "sizeY": 2,
        "type": "circular-heat-form",
        "icon": "img/visualizations/OpsClock64.png"
    }, {
        "name": "Tag Cloud",
        "sizeX": 2,
        "sizeY": 2,
        "type": "tag-cloud",
        "bindings": {
            "tag-field": "'hashtags'"
        },
        "icon": "img/visualizations/TagCloud64.png"
    }, {
        "name": "Count By",
        "sizeX": 2,
        "sizeY": 2,
        "type": "count-by",
        "icon": "img/visualizations/Count64.png"
    }, {
        "name": "Sunburst",
        "sizeX": 2,
        "sizeY": 2,
        "type": "sunburst",
        "icon": "img/visualizations/Sunburst64.png"
    }, {
        "name": "View Data",
        "sizeX": 6,
        "sizeY": 2,
        "type": "query-results-table",
        "icon": "img/visualizations/ViewData64.png"
    }, {
        "name": "Graph",
        "sizeX": 4,
        "sizeY": 2,
        "type": "directed-graph",
        "icon": "img/visualizations/Graph64.png"
    }],

    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "database": "test",
        "tables": [{
            "name": "earthquakes",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "time",
                "prettyName": "Time"
            }, {
                "columnName": "latitude",
                "prettyName": "Latitude"
            }, {
                "columnName": "longitude",
                "prettyName": "Longitude"
            }, {
                "columnName": "depth",
                "prettyName": "Depth"
            }, {
                "columnName": "mag",
                "prettyName": "Magnitude"
            }, {
                "columnName": "magType",
                "prettyName": "Magnitude Type"
            }, {
                "columnName": "nst",
                "prettyName": "NST"
            }, {
                "columnName": "gap",
                "prettyName": "Gap"
            }, {
                "columnName": "dmin",
                "prettyName": "DMIN"
            }, {
                "columnName": "rms",
                "prettyName": "RMS"
            }, {
                "columnName": "net",
                "prettyName": "Net"
            }, {
                "columnName": "id",
                "prettyName": "ID"
            }, {
                "columnName": "updated",
                "prettyName": "Updated"
            }, {
                "columnName": "place",
                "prettyName": "Place"
            }, {
                "columnName": "type",
                "prettyName": "Type"
            }],
            "mappings": {
                "date": "time",
                "latitude": "latitude",
                "longitude": "longitude",
                "line_category": "type",
                "bar_x_axis": "net",
                "y_axis": "mag",
                "color_by": "magType",
                "size_by": "mag",
                "sort_by": "time"
            }
        }]
    }],

    "layouts": {
        "default": [{
            "sizeX": 6,
            "sizeY": 1,
            "type": "timeline-selector"
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "heat-map"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by"
        }]
    }
}
