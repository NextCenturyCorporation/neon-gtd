{
    "user_ale": {
        "loggingUrl": "http://192.168.1.100",
        "toolName": "Neon Dashboard",
        "toolVersion": "1.1.0",
        "elementGroups": [
            "top",
            "map_group",
            "table_group",
            "chart_group",
            "query_group",
            "graph_group"
        ],
        "workerUrl": "lib/user-ale/js/userale-worker.js",
        "debug": false,
        "sendLogs": false
    },

    "dashboard": {
        "gridsterColumns": 6,
        "hideNavbarItems": false
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "dig": {
        "enabled": false,
        "server": "http://localhost:9000"
    },

    "visualizations": [{
        "name": "Filter Builder",
        "sizeX": 4,
        "minSizeX": 3,
        "sizeY": 2,
        "minSizeY": 1,
        "type": "filter-builder",
        "icon": "img/visualizations/CreateFilter64.png"
    }, {
        "name": "Data Table",
        "sizeX": 6,
        "sizeY": 2,
        "type": "query-results-table",
        "icon": "img/visualizations/ViewData64.png"
    }, {
        "name": "Timeline",
        "sizeX": 6,
        "sizeY": 1,
        "minSizeY": 1,
        "type": "timeline-selector",
        "icon": "img/visualizations/Timeline64.png"
    }, {
        "name": "Map",
        "sizeX": 4,
        "sizeY": 2,
        "type": "heat-map",
        "icon": "img/visualizations/Map64.png"
    }, {
        "name": "Line Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "linechart",
        "icon": "img/visualizations/LineChart64.png"
    }, {
        "name": "Bar Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "barchart",
        "icon": "img/visualizations/BarChart64.png"
    }, {
        "name": "Ops Clock",
        "sizeX": 2,
        "sizeY": 2,
        "type": "circular-heat-form",
        "icon": "img/visualizations/OpsClock64.png"
    }, {
        "name": "Text Cloud",
        "sizeX": 2,
        "sizeY": 2,
        "type": "tag-cloud",
        "icon": "img/visualizations/TagCloud64.png"
    }, {
        "name": "Count Table",
        "sizeX": 2,
        "sizeY": 2,
        "type": "count-by",
        "icon": "img/visualizations/Count64.png"
    }, {
        "name": "Sunburst Chart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "sunburst",
        "icon": "img/visualizations/Sunburst64.png"
    }, {
        "name": "Graph",
        "sizeX": 4,
        "sizeY": 2,
        "type": "directed-graph",
        "icon": "img/visualizations/Graph64.png"
    }],

    "datasets": [{
        "name": "Earthquakes",
        "datastore": "mongo",
        "hostname": "localhost",
        "database": "test",
        "tables": [{
            "name": "earthquakes",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "time",
                "prettyName": "Time"
            }, {
                "columnName": "latitude",
                "prettyName": "Latitude"
            }, {
                "columnName": "longitude",
                "prettyName": "Longitude"
            }, {
                "columnName": "depth",
                "prettyName": "Depth"
            }, {
                "columnName": "mag",
                "prettyName": "Magnitude"
            }, {
                "columnName": "magType",
                "prettyName": "Magnitude Type"
            }, {
                "columnName": "nst",
                "prettyName": "NST"
            }, {
                "columnName": "gap",
                "prettyName": "Gap"
            }, {
                "columnName": "dmin",
                "prettyName": "DMIN"
            }, {
                "columnName": "rms",
                "prettyName": "RMS"
            }, {
                "columnName": "net",
                "prettyName": "Net"
            }, {
                "columnName": "id",
                "prettyName": "ID"
            }, {
                "columnName": "updated",
                "prettyName": "Updated"
            }, {
                "columnName": "place",
                "prettyName": "Place"
            }, {
                "columnName": "type",
                "prettyName": "Type"
            }],
            "mappings": {
                "date": "time",
                "latitude": "latitude",
                "longitude": "longitude",
                "line_category": "type",
                "bar_x_axis": "net",
                "y_axis": "mag",
                "color_by": "magType",
                "size_by": "mag",
                "sort_by": "time"
            }
        }]
    }],

    "layouts": {
        "default": [{
            "sizeX": 6,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "timeline-selector"
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "heat-map"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by"
        }],

        "example-with-bindings": [{
            "sizeX": 6,
            "sizeY": 1,
            "type": "timeline-selector",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-date-field": "'date'"
            }
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "heat-map",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-latitude-field": "'latitude'",
                "bind-longitude-field": "'longitude'",
                "bind-color-field": "'type'",
                "bind-size-field": "'type'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-date-field": "'date'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-date-field": "'date'",
                "bind-y-axis-field": "'type'",
                "bind-category-field": "'type'",
                "bind-aggregation-field": "'count'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-x-axis-field": "'type'",
                "bind-y-axis-field": "'type'",
                "bind-aggregation-field": "'count'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-count-field": "'type'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "tag-cloud",
            "bindings": {
                "bind-table": "'myTable'",
                "bind-tag-field": "'hashtags'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "sunburst",
            "bindings": {
                "bind-table": "'myTable'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 2,
            "type": "query-results-table",
            "bindings": {
                "bind-table": "'myTable'"
            }
        }]
    },

    "files": {
        "datasets": [
            "./config/sample.dataset.config.json"
        ],
        "layouts": [
            "./config/sample.layout.config.json"
        ]
    }
}
