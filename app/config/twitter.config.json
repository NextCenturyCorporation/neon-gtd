    "datasets": [{
        "name": "Neon-Tangelo Demo",
        "layout": "mentions",
        "datastore": "mongo",
        "hostname": "192.168.0.129",
        "database": "test",
        "tables": [{
            "name": "twitter36_reimport",
            "fields": [],
            "mappings": {
                "date": "created_at",
                "latitude": "latitude",
                "longitude": "longitude",
                "color_by": "lang",
                "size_by": "user_followers_count",
                "bar_x_axis": "lang",
                "count_by": "screen_name_tolower"
            }
        }, {
            "name": "twitter_mentions",
            "fields": [],
            "mappings": {
                "date": "created_at",
                "graph_nodes": "source",
                "bar_x_axis": "source",
                "count_by": "source"
            }
        }],
        "relations": [{
            "twitter36_reimport": "created_at",
            "twitter_mentions": "date"
        }, {
            "twitter36_reimport": "screen_name_tolower",
            "twitter_mentions": "source"
        }, {
            "twitter36_reimport": "screen_name_tolower",
            "twitter_mentions": "target"
        }],
