{
    "user_ale": {
        "loggingUrl": "http://192.168.1.100",
        "toolName": "Neon Dashboard",
        "toolVersion": "1.1.0",
        "elementGroups": [
            "top",
            "map_group",
            "table_group",
            "chart_group",
            "query_group",
            "graph_group"
        ],
        "workerUrl": "lib/user-ale/js/userale-worker.js",
        "debug": false,
        "sendLogs": false
    },

    "dashboard": {
        "gridsterColumns": 6,
        "hideNavbarItems": false
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "dig": {
        "enabled": true,
        "server": "http://10.1.93.199:8080",
        "count_by": {
            "name": "DIG",
            "image": "img/DIG_64.png",
            "url": "SERVER/list",
            "args": [{
                "name": "field",
                "value": "FIELD"
            }, {
                "name": "value",
                "value": "VALUE"
            }]
        },
        "data_table": {
            "name": "DIG",
            "image": "img/DIG_64.png",
            "url": "SERVER/list",
            "args": [{
                "name": "id",
                "value": "VALUE"
            }]
        }
    },

    "quickpin": {
        "enabled": true,
        "server": "http://10.1.93.174:5000",
        "count_by": {
            "name": "Quickpin",
            "image": "img/Quickpin_64.png",
            "url": "SERVER/details/VALUE",
            "args": []
        },
        "data_table": {
            "name": "Quickpin",
            "image": "img/Quickpin_64.png",
            "url": "SERVER/details/VALUE",
            "args": []
        }
    },

    "visualizations": [{
        "name": "Filter Builder",
        "sizeX": 4,
        "minSizeX": 3,
        "sizeY": 2,
        "minSizeY": 1,
        "type": "filter-builder",
        "icon": "img/visualizations/CreateFilter64.png"
    }, {
        "name": "Data Table",
        "sizeX": 6,
        "sizeY": 2,
        "type": "query-results-table",
        "icon": "img/visualizations/ViewData64.png"
    }, {
        "name": "Timeline",
        "sizeX": 6,
        "sizeY": 1,
        "minSizeY": 1,
        "type": "timeline-selector",
        "icon": "img/visualizations/Timeline64.png"
    }, {
        "name": "Map",
        "sizeX": 4,
        "sizeY": 2,
        "type": "heat-map",
        "icon": "img/visualizations/Map64.png"
    }, {
        "name": "Linechart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "linechart",
        "icon": "img/visualizations/LineChart64.png"
    }, {
        "name": "Barchart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "barchart",
        "icon": "img/visualizations/BarChart64.png"
    }, {
        "name": "Ops Clock",
        "sizeX": 2,
        "sizeY": 2,
        "type": "circular-heat-form",
        "icon": "img/visualizations/OpsClock64.png"
    }, {
        "name": "Tag Cloud",
        "sizeX": 2,
        "sizeY": 2,
        "type": "tag-cloud",
        "icon": "img/visualizations/TagCloud64.png"
    }, {
        "name": "Count By",
        "sizeX": 2,
        "sizeY": 2,
        "type": "count-by",
        "icon": "img/visualizations/Count64.png"
    }, {
        "name": "Sunburst",
        "sizeX": 2,
        "sizeY": 2,
        "type": "sunburst",
        "icon": "img/visualizations/Sunburst64.png"
    }, {
        "name": "Graph",
        "sizeX": 4,
        "sizeY": 2,
        "type": "directed-graph",
        "icon": "img/visualizations/Graph64.png"
    }, {
        "name": "Mentions",
        "sizeX": 6,
        "sizeY": 3,
        "type": "tangelo-mentions",
        "icon": "js/vendor/tangelo-mentions/tangelo-64x64.png"
    }],

    "datasets": [{
        "name": "QCR",
        "layout": "qcr",
        "datastore": "mongo",
        "hostname": "localhost",
        "database": "test",
        "tables": [{
            "name": "twitter36",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "created_at",
                "prettyName": "Created At"
            }, {
                "columnName": "screen_name",
                "prettyName": "Screen Name"
            }, {
                "columnName": "lang",
                "prettyName": "Language"
            }, {
                "columnName": "hashtags",
                "prettyName": "Hashtags"
            }, {
                "columnName": "text",
                "prettyName": "Text"
            }],
            "mappings": {}
        }, {
            "name": "ff",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "attributeList",
                "prettyName": "Attribute List"
            }, {
                "columnName": "id",
                "prettyName": "User ID"
            }, {
                "columnName": "label",
                "prettyName": "Label"
            }, {
                "columnName": "magnitude",
                "prettyName": "Magnitude"
            }],
            "mappings": {
                "graph_nodes": "label",
                "graph_links": "attributeList"
            }
        }],
        "relations": [{
            "twitter36": "screen_name",
            "ff": "label"
        }],
        "connectOnLoad": false
    },

    {
        "name": "Neon & Twitter Mentions",
        "layout": "mentions",
        "datastore": "mongo",
        "hostname": "192.168.0.129",
        "database": "test",
        "tables": [{
            "name": "twitter36_reimport",
            "fields": [],
            "mappings": {
                "date": "created_at",
                "latitude": "latitude",
                "longitude": "longitude",
                "color_by": "lang",
                "size_by": "user_followers_count",
                "bar_x_axis": "lang",
                "tags": "hashtags",
                "count_by": "screen_name_tolower"
            }
        }, {
            "name": "twitter_mentions",
            "fields": [],
            "mappings": {
                "date": "created_at",
                "graph_nodes": "source",
                "bar_x_axis": "source",
                "count_by": "source"
            }
        }],
        "relations": [{
            "twitter36_reimport": "created_at",
            "twitter_mentions": "date"
        }, {
            "twitter36_reimport": "screen_name_tolower",
            "twitter_mentions": "source"
        }, {
            "twitter36_reimport": "screen_name_tolower",
            "twitter_mentions": "target"
        }],
        "connectOnLoad": false
    }],

    "layouts": {
        "default": [{
            "sizeX": 6,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "timeline-selector"
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "heat-map"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by"
        }],

        "qcr": [{
            "sizeX": 6,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "timeline-selector",
            "bindings": {
                "bind-date-field": "'created_at'"
            }
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "heat-map",
            "bindings": {
                "bind-latitude-field": "'latitude'",
                "bind-longitude-field": "'longitude'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "circular-heat-form",
            "bindings": {
                "bind-date-field": "'created_at'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "linechart",
            "bindings": {
                "bind-date-field": "'created_at'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "barchart",
            "bindings": {
                "bind-x-axis-field": "'lang'"
            }
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by",
            "bindings": {
                "bind-count-field": "'screen_name'"
            }
        }, {
            "sizeX": 4,
            "sizeY": 2,
            "type": "directed-graph"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "minSizeY": 1,
            "type": "tag-cloud",
            "bindings": {
                "bind-tag-field": "'hashtags'"
            }
        }, {
            "sizeX": 6,
            "sizeY": 2,
            "type": "query-results-table"
        }],

        "mentions": [{
            "sizeX": 6,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "timeline-selector"
        }, {
            "sizeX": 4,
            "sizeY": 3,
            "type": "tangelo-mentions"
        }, {
            "sizeX": 2,
            "sizeY": 1,
            "minSizeY": 1,
            "type": "tag-cloud"
        }, {
            "sizeX": 2,
            "sizeY": 2,
            "type": "count-by"
        }, {
            "sizeX": 6,
            "sizeY": 2,
            "type": "query-results-table"
        }]
    }
}
