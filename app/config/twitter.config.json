{
    "xdata": {
        "echoToConsole": false,
        "enableLoggin": false,
        "activityLoggerUrl": "http://xd-draper.xdata.data-tactics-corp.com:1337",
        "component": "Neon Demo",
        "componentVersion": "1.0.0-SNAPSHOT"
    },

    "opencpu": {
        "enableOpenCpu": false,
        "enableLogging": false,
        "useAlerts": false,
        "url": "http://neon-opencpu/ocpu/library/NeonAngularDemo/R"
    },

    "dig": {
        "enabled": false,
        "server": "http://localhost:9000"
    },

    "visualizations": [{
        "name": "Timeline",
        "sizeX": 6,
        "sizeY": 1,
        "type": "timeline-selector",
        "icon": "img/visualizations/Timeline64.png"
    }, {
        "name": "Map",
        "sizeX": 4,
        "sizeY": 2,
        "type": "heat-map",
        "icon": "img/visualizations/Map64.png"
    }, {
        "name": "Linechart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "linechart",
        "icon": "img/visualizations/LineChart64.png"
    }, {
        "name": "Barchart",
        "sizeX": 2,
        "sizeY": 2,
        "type": "barchart",
        "icon": "img/visualizations/BarChart64.png"
    }, {
        "name": "Ops Clock",
        "sizeX": 2,
        "sizeY": 2,
        "type": "circular-heat-form",
        "icon": "img/visualizations/OpsClock64.png"
    }, {
        "name": "Tag Cloud",
        "sizeX": 2,
        "sizeY": 2,
        "type": "tag-cloud",
        "bindings": {
            "tag-field": "'hashtags'"
        },
        "icon": "img/visualizations/TagCloud64.png"
    }, {
        "name": "Count By",
        "sizeX": 2,
        "sizeY": 2,
        "type": "count-by",
        "icon": "img/visualizations/Count64.png"
    }, {
        "name": "Sunburst",
        "sizeX": 2,
        "sizeY": 2,
        "type": "sunburst",
        "icon": "img/visualizations/Sunburst64.png"
    }, {
        "name": "Data Table",
        "sizeX": 6,
        "sizeY": 2,
        "type": "query-results-table",
        "icon": "img/visualizations/ViewData64.png"
    }, {
        "name": "Graph",
        "sizeX": 4,
        "sizeY": 2,
        "type": "directed-graph",
        "icon": "img/visualizations/Graph64.png"
    }, {
        "name": "Mentions",
        "sizeX": 6,
        "sizeY": 2,
        "type": "tangelo-mentions",
        "icon": "js/vendor/tangelo-mentions/tangelo-64x64.png"
    }],

    "datasets": [{
        "name": "Local Twitter Test",
        "layout": "qcr",
        "datastore": "mongo",
        "hostname": "localhost",
        "database": "test",
        "tables": [{
            "name": "twitter36",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "created_at",
                "prettyName": "Created At"
            }, {
                "columnName": "screen_name",
                "prettyName": "Screen Name"
            }, {
                "columnName": "lang",
                "prettyName": "Language"
            }, {
                "columnName": "hashtags",
                "prettyName": "Hashtags"
            }, {
                "columnName": "text",
                "prettyName": "Text"
            }],
            "mappings": {
                "date": "created_at",
                "count_by": "screen_name"
            }
        }, {
            "name": "ff",
            "fields": [{
                "columnName": "_id",
                "prettyName": "Database ID"
            }, {
                "columnName": "attributeList",
                "prettyName": "Attribute List"
            }, {
                "columnName": "id",
                "prettyName": "User ID"
            }, {
                "columnName": "label",
                "prettyName": "Label"
            }, {
                "columnName": "magnitude",
                "prettyName": "Magnitude"
            }],
            "mappings": {
                "graph_nodes": "label"
            }
        }],
        "relations": [{
            "twitter36": "screen_name",
            "ff": "label"
        }],
        "connectOnLoad": true
    },

