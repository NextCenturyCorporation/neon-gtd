{
    "name": "neon-gtd",
    "description": "A sample geo-temporal analysis dashboard that uses the neon.js library to show interactions between multiple visualizations of a dataset.",
    "version": "1.1.0-SNAPSHOT",
    "homepage": "https://github.com/NextCenturyCorporation/neon-gtd",
    "license": "Apache v2",
    "private": true,
    "dependencies": {
        "angular": "1.2.26",
        "angular-bootstrap-datetimepicker": "0.3.8",
        "angular-gridster": "0.11.0",
        "angular-loader": "1.2.26",
        "angular-mocks": "1.2.26",
        "angular-route": "1.2.26",
        "bootstrap": "3.2.0",
        "d3": "3.0.8",
        "html5shiv": "3.7.2",
        "jquery": "1.11.1",
        "jquery-ui": "1.10.4",
        "threedubmedia.jquery.event": "2.2",
        "javascript-detect-element-resize": "sdecima/javascript-detect-element-resize#0.5.3",
        "jquery.tagcloud": "https://github.com/addywaddy/jquery.tagcloud.js.git",
        "jquery.linky": "0.1.8",
        "mergesort": "https://github.com/justinforce/merge-sort/archive/1.0.zip",
        "moment": "2.10.3",
        "opencpu": "https://github.com/NextCenturyCorporation/opencpu.js.git",
        "openlayers": "https://github.com/openlayers/openlayers/releases/download/release-2.13.1/OpenLayers-2.13.1.zip",
        "heatmapjs": "https://github.com/pa7/heatmap.js.git#v2.0",
        "remarkable-bootstrap-notify": "3.0.0",
        "respond": "1.4.2",
        "slickgrid": "2.1.0",
        "user-ale": "draperlaboratory/user-ale#67954dc8bcb02c657306ea89193b25e4831bf59e"
    },
    "resolutions": {
        "jquery": "1.11.x"
    },
    "exportsOverride": {
        "angular-mocks": {
            "js": "**/*.js",
            "css": "**/*.css"
        },

        "bootstrap-*": {
            "js": "**/*.js",
            "css": "**/*.css"
        },

        "bootstrap": {
                "js": "dist/js/*.js",
                "css": "dist/css/*.css",
                "fonts": "dist/fonts/*.*"
        },

        "d3": {
            "js": "**/d3.min.js",
            "css": "**/*.css"
        },

        "html5shiv": {
            "js": "**/*min.js"
        },

        "threedubmedia.jquery.event": {
            "js": "**/jquery.event*.js",
            "css": "**/*.css"
        },

        "javascript-detect-element-resize": {
            "js": "**/*.js"
        },

        "jquery.tagcloud": {
            "js": "**/*.js",
            "css": "**/*.css"
        },

        "jquery-ui": {
            "js": "**/jquery-ui.min.js",
            "themes/smoothness": "themes/smoothness/**.css",
            "themes/smoothness/images": ["themes/smoothness/images/*.png","themes/smoothness/images/*.gif"],
            "themes/ui-lightness": "themes/ui-lightness/**.css",
            "themes/ui-lightness/images": ["themes/ui-lightness/images/*.png","themes/ui-lightness/images/*.gif"]
        },

        "mergesort": {
            "js": "**/merge*.js"
        },

        "opencpu": {
            "js": "**/opencpu-*.*.js"
        },

        "openlayers": {
            ".": ["OpenLayer*.js", "OpenLayers.*.js"],
            "theme/default": "theme/default/*.css",
            "theme/default/img": ["**/*.gif", "**/*.png"],
            "img": ["img/*.gif", "img/*.png"]
        },

        "openlayers-heatmap": {
            "js": ["**/heatmap.js", "**/*openlayers*.js"]
        },

        "respond": {
            "js": "dest/*min.js"
        },

        "slickgrid": {
            "js": "**/slick.*.js",
            "css/images": ["**/*.gif", "**/*.png"],
            "css": "**/slick*.css"
        },

        "user-ale": {
            "js": "helper-libs/javascript/*.js"
        },

        "*": {
            "js": ["**/*min.js", "**/*.map"],
            "css": "**/*.css"
        }
    }

}
